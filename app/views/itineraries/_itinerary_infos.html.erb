<div class="nested-fields">
  <div class ="card-panel">
  <div class ="row">

    <div class="field">
      <div class="input-field col s1 valign-wrapper">
        <i class="mdi-editor-format-line-spacing small grey-text"></i>
        <h6 class="grey-text valign iti_postion_number"></h6>
      </div>
      
      <div class="input-field col s1">
      <%= f.select(:product_id, options_for_select([[f.object.get_product_name, f.object.product_id]],f.object.product_id),{},{class: "select2-products browser-default"}) %>
      </div>
      
      <%= f.hidden_field :position, :value => f.object.position, class: "iti_position_field" %>

      <div class="input-field col s1">
          <%= f.date_field :start_date, class:"datepicker", :"data-value" =>  (f.object.start_date.strftime("%Y/%m/%d") unless f.object.start_date.nil?) %>
          <%= f.label :start_date %>
      </div>

      <div class="input-field col s1">
          <%= f.date_field :end_date, class:"datepicker", :"data-value" =>  (f.object.end_date.strftime("%Y/%m/%d") unless f.object.end_date.nil?) %>
          <%= f.label :end_date %>
      </div>
      
      <div class="input-field col s1">
          <%= f.text_field :country %>
          <%= f.label :country %>
      </div>

      <div class="input-field col s1">
          <%= f.text_field :city %>
          <%= f.label :city %>
      </div>
      
      <div class="input-field col s1">
          <%= f.text_field :product_type %>
          <%= f.label :product_type, "Type" %>
      </div>
      
      <div class="input-field col s1">
          <%= f.text_field :product_name %>
          <%= f.label :product_name, "Name" %>
      </div>
      
      <div class="input-field col s1">
          <%= f.text_field :rating %>
          <%= f.label :rating %>
      </div>
      <div class="input-field col s1">
          <%= f.text_field :room_type %>
          <%= f.label :room_type %>
      </div>
      
      <div class="input-field col s1 product_details_cont">
        <%= text_field_tag(:supplier_name, f.object.supplier_name, class: "supplier_name active", :readonly => true  ) %>
        <%= f.label :supplier_name, "Supplier" %>
      </div>
     </div>
     
     
    <div class="input-field col s1">
      <%= link_to_remove_association  f, { class: "btn-floating btn-small waves-effect waves-light red right"} do %>
        <i class="mdi-content-clear"></i>
      <% end %>
    </div>

  </div>
</div>
</div>