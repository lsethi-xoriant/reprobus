<div class="panel panel-default">
  <div class="panel-heading">
    Current Overrides
  </div>
  <!-- /.panel-heading -->
  <div class="panel-body">
    <!-- <table id="sort_table" class="table table-striped table-bordered table-hover" > -->
    <table class="table table-striped table-bordered table-hover" >
      <thead>
        <tr>
          <th>Code</th>
          <th>Rate Set</th>
          <th>Todays Actual Rate</th>
        </tr>
      </thead>
      <tbody>
        <% @setting.exchange_rates.each do |ex| %>
        <tr>
          <td><%= ex.currency_code %></td>
          <td><%= ex.exchange_rate %></td>
          <td><%= ex.getTodayExRate %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
 </div>