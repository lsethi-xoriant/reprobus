<% provide(:title, @user.name + " Dashboard" ) %>


  <% if @user.assigned_enquiries.any? %>
    <% @user.assigned_enquiries.each do |enquiry| %>
      <%= render partial: 'enquiries/dashboard_enquiry', locals: { enquiry: enquiry } %>
    <% end %>
  <% else %>
      <h3>No Enquiries</h3>
  <% end %>




