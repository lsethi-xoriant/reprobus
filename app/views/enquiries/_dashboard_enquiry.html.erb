<!-- Start card -->
<div class="block span3 block-head-btn new">
  <p class="block-heading"> <%= enquiry.dasboard_customer_name %>             
  <span class="pull-right">
    <button class="btn btn-primary" href= <%= enquiry_path enquiry %> data-toggle="modal">
      <i class="icon-cog"></i>
    </button>
  </span>          
  </p> 
  
  <div class="block-body">
    <p> <span class="text-primary"><i class="icon-envelope-alt"> </i></span><span class="body"> <%= enquiry.customer_email %> </span></p>
    <p> <span class="text-primary"><i class="icon-tag"> </i>  </span><span class="body"> <%= enquiry.name %></span></p>
    <p> <span class="text-primary"><i class="icon-map-marker"> </i> </span><span class="body"> <%= enquiry.name %> </span></p>
    <p> <span class="text-primary"><i class="icon-comments"> </i> </span><span class="label label-danger">Email, 7 March 2014</span> </p>
    <p> <span class="text-primary"><i class="icon-money"> </i></span><span class="body"><%= number_to_currency(enquiry.amount, precision: 0) %></span></p>
    <div class="progress">
       <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: 20%;"> 20%
      </div>
    </div>
  </div>
</div>



  

<!-- End card --> 
