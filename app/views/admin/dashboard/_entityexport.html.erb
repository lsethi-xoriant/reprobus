<% provide(:title, "Export " + entity.name.pluralize) %>
<div class="row-fluid">
  <div class="page-header">
    <h1>Export <%=entity.name.pluralize %></h1>

    <%= render partial: 'admin/dashboard/tableheader', locals: { entity: entity, state: "export" } %>
    <br>
    <br>
    <a href="/admin/<%=entity.name.pluralize.downcase%>.csv" >
    <button class="btn btn-primary" name="csv">
    <i class="icon-white icon-ok"></i>
    Export to csv
    </button></a>

    <a href="/admin/<%=entity.name.pluralize.downcase%>.xls" >
    <button class="btn btn-primary" name="xls">
    <i class="icon-white icon-ok"></i>
    Export to Excel
    </button></a>
    <br><br>
  </div>
</div>