<% provide(:title, 'Edit Carrier') %>
<h1 class="page-header">Edit a Carrier</h1>
<div class="row">
  <div class="span6 offset3">
    <%#= form_for @carrier, :url => admin_carrier_path(@carrier), :html => {:multipart => true, :class => "form-horizontal"} do |f| %>
  <%= form_for [:admin, @carrier], :html => {:multipart => true, :class => "form-horizontal"} do |f| %>
	    <%= render 'shared/error_messages', object: f.object %>
    
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.submit "Edit a Carrier", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
