<% provide(:title, 'Edit Stopover') %>
<h1 class="page-header">Edit a Stopover</h1>
<div class="row">
  <div class="span6 offset3">
    <%#= form_for @stopover, :url => admin_stopover_path(stopover), :html => {:multipart => true} do |f| %>
  <%= form_for [:admin, @stopover], :html => {:multipart => true} do |f| %>
	    <%= render 'shared/error_messages', object: f.object %>
    
    <%= f.label :name %>
    <%= f.text_field :name %>
   
    <%= f.submit "Edit a Stopover", class: "btn btn-large btn-primary" %>
   
    <% end %>
  </div>
</div>